@model TecolocoClient.Models.Jobs
@{
    ViewBag.Title = "DetailJob";
}

<h2>Detail Job</h2>
<hr />
<div class="form-group">
    <div class="form-group">
        <label>IdJob</label>
        @Model.JobID
    </div>
    <div class="form-group">
        <label>Job</label>
        @Model.JobName
    </div>

    <div class="form-group">
        <label>JobTitle</label>
        @Model.JobTitle
    </div>
    <div class="form-group">
        <label>Description</label>
        @Model.Description
    </div>
    <div class="form-group">
        <label>CreateAt</label>
        @Model.CreateAt
    </div>
    <div class="form-group">
        <label>ExpiresAt</label>
        @Model.ExpiresAt
    </div>
    <div class="form-group">
        <a href="@Url.Action("Index","Jobs")">Return</a>
    </div>

</div>
